{
    "routes": [
        {
            "route": "/MazeGame1",
            "serve": "MazeGame1/index.html"
        },
        {
            "route": "/MazeGame1/*",
            "serve": "MazeGame1/*"
        },
        {
            "route": "/myFruitGame1",
            "serve": "myFruitGame1/index.html"
        },
        {
            "route": "/myFruitGame1/*",
            "serve": "myFruitGame1/*"
        },
        {
            "route": "/myFruitGame2",
            "serve": "myFruitGame2/index.html"
        },
        {
            "route": "/myFruitGame2/*",
            "serve": "myFruitGame2/*"
        }
    ],
    "navigationFallback": {
        "rewrite": "index.html"
    },
    "globalHeaders": {
        "content-security-policy": "default-src 'self' 'unsafe-inline' 'unsafe-eval' https://cdn.tailwindcss.com;"
    }
}